<MudStack Spacing="0">
    <MudText GutterBottom Typo="Typo.subtitle1" Color="Color.Secondary">@Title</MudText>
    <MudText Typo="Typo.subtitle2" Color="Color.Default">@Subtitle</MudText>
</MudStack>

@code {
    [Parameter]
    public string? Title { get; set; }
    [Parameter]
    public string? Subtitle { get; set; }
}
